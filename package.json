{
  "name": "@asl-19/next-utils",
  "version": "0.1.0",
  "description": "Shared Next.js utility functions for ASL19 projects",
  "author": {
    "name": "Grant Heaslip",
    "email": "grant@asl19.org",
    "url": "https://asl19.org/"
  },
  "license": "GPL-3.0-or-later",
  "repository": "ASL-19/next-utils",
  "engines": {
    "node": ">= 16"
  },
  "files": [
    "dist/**/*.js",
    "types/**/*.d.ts"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "types/next-utils.d.ts",
  "sideEffects": false,
  "scripts": {
    "api-extractor:check": "api-extractor run --verbose",
    "api-extractor:update": "api-extractor run --local --verbose",
    "api-documenter:update": "api-documenter markdown --input-folder ./api-extractor --output-folder ./docs",
    "build": "yarn clean-dist && concurrently --names='build-cjs,build-esm' 'yarn build-cjs' 'yarn build-esm'",
    "build-cjs": "tsc --project tsconfig-cjs.json",
    "build-esm": "tsc --project tsconfig-esm.json",
    "build-and-update-api": "yarn build && yarn api-extractor:update && yarn api-documenter:update",
    "clean-dist": "rm -rf ./dist/",
    "eslint": "eslint . --ext js,ts",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {},
  "devDependencies": {
    "@asl-19/eslint-config": "^0.4.0",
    "@emotion/react": "^11.4.0",
    "@microsoft/api-documenter": "^7.13.30",
    "@microsoft/api-extractor": "^7.18.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^6.2.0",
    "eslint": "^7.30.0",
    "jest": "^27.0.6",
    "next": "^11.0.2-canary.9",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "next": "^11.0.2-canary.7"
  }
}
